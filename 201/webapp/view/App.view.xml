<mvc:View
	controllerName="sap.ui5.walkthrough.controller.app"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:ce="sap.ui.codeeditor"
	height="100%">
	<IconTabHeader
		id="iconTabHeader"
		selectedKey="invalidKey"
		select=".onSelectTab">
		<items>
			<IconTabFilter text="XML view" key="xml" />
			<IconTabFilter text="Controller" key="cont" />
		</items>
	</IconTabHeader>
	<ce:CodeEditor id="aCodeEditor" height="300px" type="javascript" />
</mvc:View>